#########################################################################
# 3. CFLAGS
#########################################################################

CFLAGS := -O2 -DDMACAHB_DEBUG -DCONFIG_OTHER_TRANS_TYPE_TEST=1 -Wno-int-to-pointer-cast -g

# CFLAGS += -DUSE_RTOS
# CFLAGS += -DRTOS_RTTHREAD

#########################################################################
# 5. define build target
#########################################################################

TARGET := fpsoc_dmacahb_other_trans_type

ifneq ($(PINMUX_MODE),)
CFLAGS += -DENABLE_$(PINMUX_MODE)=1
TARGET:=$(TARGET)_$(CHIP)_$(PINMUX_MODE)
endif

all: $(TARGET)
	@$(ECHO) COMPILE_PREFIX = $(COMPILE_PREFIX)

# include rules.mk
include $(SDK_ROOT)/tools/make/rules.mk