#########################################################################
# 3. CFLAGS
# AL9000_OSC_33: for chip pll enable and call clk_simple_config func
# AL9000_BRINGUP: for chip bypass mpu disable flow
# BOARD_DR1X90_AD101_V10: for demo board select uart 1 as log output
# AUTO_TEST: for emulation platform auto test config
# USE_DDR: for test case read/write buffer in ddr
# CONFIG_PERFORMANCE_CALC: for performace calc
#########################################################################
CFLAGS := -O2 -DMMC_DEBUG -DCONFIG_EMMC_TEST=1 -g
CFLAGS += -DAL9000_OSC_33
CFLAGS += -DAL9000_BRINGUP
CFLAGS += -DBOARD_DR1X90_AD101_V10
# CFLAGS += -DAUTO_TEST
# CFLAGS += -DUSE_DDR
# CFLAGS += -DCONFIG_PERFORMANCE_CALC

#########################################################################
# 5. define build target
#########################################################################

TARGET := fpsoc_mmc_emmc

ifneq ($(PINMUX_MODE),)
CFLAGS += -DENABLE_$(PINMUX_MODE)=1
TARGET:=$(TARGET)_$(CHIP)_$(PINMUX_MODE)
endif

all: $(TARGET)
	@$(ECHO) COMPILE_PREFIX = $(COMPILE_PREFIX)

# include rules.mk
include $(SDK_ROOT)/tools/make/rules.mk